{
    "name": "allanmcarvalho/base-two-acl",
    "description": "Plugin for manage Access Control Level (ACL) for CakePHP 3 using the base 2 math power",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.0.0 <4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "BaseTwoACL\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BaseTwoACL\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
